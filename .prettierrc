{
  "overrides": [
    {
      "files": "*.latte",
      "options": {
        "parser": "html"
      }
    }
  ],
  "printWidth": 100,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": false,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "htmlWhitespaceSensitivity": "ignore"
}
